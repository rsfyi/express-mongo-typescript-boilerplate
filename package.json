	"dependencies": {
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.20.1",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"cross-env": "^5.2.0",
		"csurf": "^1.10.0",
		"express": "^4.18.2",
		"express-rate-limit": "^4.0.4",
		"helmet": "^3.18.0",
		"jsonwebtoken": "^8.5.1",
