{
	"name": "express-mongo-typescript-boilerplate",
	"version": "1.0.0",
	"description": "This repository is basic setup of expressjs, mongodb with typescript, feel free to fork and create pr",
	"main": "index.js",
	"scripts": {
		"server-development": "./node_modules/.bin/ts-node ./server/server.ts",
		"start": "./node_modules/.bin/cross-env NODE_ENV=development ./node_modules/.bin/nodemon -w ./server --ext \".ts\" --exec \"npm run server-development\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/rsdoc/express-mongo-typescript-boilerplate.git"
	},
	"keywords": [
